{
  "manifest_version": 3,
  "name": "Browser Activity Monitor",
  "version": "1.0",
  "description": "Logs postcode and EPC URLs",
  "permissions": ["scripting", "tabs", "storage"],
  "host_permissions": ["https://www.rightmove.co.uk/properties/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
